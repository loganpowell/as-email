    <mj-section background-color="#373334" padding="48px">
      <mj-column padding="0">
        <mj-text
          font-size="16px"
          line-height="1.5"
          padding="0px"
          color="white"
          align="center"
        >
          Watch the interview and find out how they did it:
        </mj-text>
        <mj-spacer height="24px"></mj-spacer>
        <mj-button
          background-color="white"
          border-radius="32px"
          font-size="20px"
          line-height="1.5"
          padding="0px"
          width="100%"
        >
          <strong
            ><a href="https://anotherstory.com">AnotherStory.com</a></strong
          >
        </mj-button>
      </mj-column>
    </mj-section>
    <mj-section
      background-color="#373334"
      border-bottom="1px solid #5B5677"
      border-left="none"
      border-right="none"
      border-top="none"
      padding-bottom="20px"
      padding-left="48px"
      padding-right="48px"
      padding-top="20px"
      padding="48px"
    >
      <mj-column padding="0">
        <mj-image
          align="left"
          padding-bottom="4px"
          padding-left="0px"
          padding-right="0px"
          padding-top="4px"
          padding="0px"
          src="./assets/transparent-logo-white.png"
          width="28px"
        ></mj-image>
      </mj-column>
      <mj-column padding="0">
        <mj-social
          align="right"
          font-size="15px"
          icon-padding="3px"
          icon-size="36px"
          inner-padding="4px"
          mode="horizontal"
          padding="0px"
        >
          <mj-social-element
            src="https://cdn.getvero.com/dd-editor/ui-assets/icons/social/facebook.png"
            alt="https://www.facebook.com/anotherstorybham"
            border-radius="10px"
            href="https://www.facebook.com/anotherstorybham"
            name="facebook-noshare"
            title="https://www.facebook.com/anotherstorybham"
          ></mj-social-element>
          <mj-social-element
            src="https://cdn.getvero.com/dd-editor/ui-assets/icons/social/instagram.png"
            alt="https://www.instagram.com/anotherstorybham"
            border-radius="10px"
            href="https://www.instagram.com/anotherstorybham"
            name="twitter-noshare"
            title="https://www.instagram.com/anotherstorybham"
          ></mj-social-element>
        </mj-social>
      </mj-column>
    </mj-section>
    <mj-section
      padding-bottom="64px"
      padding-left="48px"
      padding-right="48px"
      padding-top="64px"
      padding="48px"
    >
      <mj-column padding="0">
        <mj-text
          color="#fff"
          font-size="14px"
          line-height="1.5"
          padding="0px"
          align="center"
        >
          <a
            ses:tags="unsubscribeLinkTag:optout"
            href="https://www.example.com/preferences"
            color="#fff"
            >opt out from future emails</a
          >
        </mj-text>
      </mj-column>
    </mj-section>
