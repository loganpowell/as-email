      background-color="#373334"
      padding-bottom="0px"
      padding-left="48px"
      padding-right="48px"
      padding-top="0px"
      padding="48px"
    >
      <mj-column padding="0">
        <mj-image
          align="right"
          padding="0px"
          src="./assets/transparent-logo-white.png"
          width="60px"
        ></mj-image>
      </mj-column>
    </mj-section>
