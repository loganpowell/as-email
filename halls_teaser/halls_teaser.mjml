<mjml>
  <mj-head>
    <mj-attributes>
      <mj-all
        font-family="'Poppins', 'Helvetica', 'Arial', sans-serif"
      ></mj-all>
      <mj-text color="#373334"></mj-text>
    </mj-attributes>
    <mj-font
      href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,400;1,900&display=swap"
      name="Poppins"
    ></mj-font>
  </mj-head>
  <mj-body background-color="#F6F6EE">
